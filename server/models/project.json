{
  "name": "project",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "content": {
      "type": "string",
      "mysql": {
        "dataType": "LONGTEXT"
      }
    },
    "shortDescription": {
      "type": "string"
    },
    "thumbUrl": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "canDonateMore": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "string",
      "default": "draft"
    },
    "contractAddress": {
      "type": "string"
    },
    "duration": {
      "type": "number"
    },
    "coinsAddresses": {
      "type": "string"
    },
    "dueDate": {
      "type": "date",
      "dataType": "timestamp"
    },
    "public": {
      "type": "boolean",
      "default": false
    },
    "published": {
      "type": "boolean",
      "default": false
    },
    "timeCreated": {
      "type": "number"
    },
    "withdraw": {
      "type": "boolean",
      "default": false
    },
    "donationStatus": {
      "type": "string",
      "default": "new"
    },
    "category": {
      "type": "string"
    },
    "authorName": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "transactions": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": ""
    },
    "accounts": {
      "type": "hasMany",
      "model": "account",
      "foreignKey": "projectId",
      "through": "transaction"
    },
    "updates": {
      "type": "hasMany",
      "model": "update",
      "foreignKey": ""
    },
    "projectStats": {
      "type": "hasOne",
      "model": "project_stats",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
