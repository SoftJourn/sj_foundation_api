{
  "name": "transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "amount": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "default": "pledged"
    },
    "date": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "project": {
      "type": "belongsTo",
      "model": "project",
      "foreignKey": ""
    },
    "account": {
      "type": "belongsTo",
      "model": "account",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
